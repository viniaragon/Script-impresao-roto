{
    "name": "echolink-project",
    "version": "1.0.0",
    "private": true,
    "description": "Sistema de Impressão Remota - EchoLink. IMPORTANTE: Cada módulo (backend, frontend, agent) é independente e tem seu próprio package.json e package-lock.json. NÃO use npm workspaces.",
    "scripts": {
        "dev:backend": "cd backend && npm run dev",
        "dev:agent": "cd agent && npm run dev",
        "dev:frontend": "cd frontend && npm run dev",
        "install:backend": "cd backend && npm install",
        "install:agent": "cd agent && npm install",
        "install:frontend": "cd frontend && npm install"
    },
    "keywords": [
        "printing",
        "remote",
        "websocket"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    }
}